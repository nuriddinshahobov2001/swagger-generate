{
    "name": "zintel/swagger-generate",
    "description": "A custom Swagger documentation generator for Laravel",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^11.0|^12.0",
        "symfony/yaml": "^7.0",
        "laravel/framework": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Zintel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Zintel\\SwaggerGenerateServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan package:discover"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
